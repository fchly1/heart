		<script type="text/javascript">
			jQuery.noConflict();
			(function($) {
				$(document).ready(function() {

					var imgIndex1 = 0,
						Interval1;
					var btnlength = $("#btnlist1 li").length;
					
					$("#btnlist1").on("click", "li", function() {
						clearInterval(Interval1);
						imgIndex1 = $(this).index();
						if(imgIndex1 >= btnlength-1){
							--imgIndex1;
						}else{
							++imgIndex1;
						}

						slide();
					}).children(":eq(0)").addClass("act");

					function slide() {
						$("#btnlist1 li").removeClass("act").eq(imgIndex1).addClass("act");
						$(".part1 .list").stop(true, false).animate({
							marginLeft: -420 * imgIndex1
						}, function() {
							$(".list-left").eq(imgIndex1).siblings().find(".shipin").css({
								display: "none"
							});
						})
					}
					$(".part1 .list-left").each(function() {
						$(this).find(".icon").click(function() {
							$(this).siblings().find(".shipin").css({
								display: "none"
							});
							$(this).parent().next(".shipin").show();
						})
					})

					/*   $(".part1 .video .list-zhezhao .icon").click(function(){
					       $(".part1 .video .list-zhezhao ").hide()
					       $(".part1 .video .shipin").show()
					   })   */

